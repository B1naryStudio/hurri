script#statistic-template(type='text/template')
	div#statistic-title STATISTIC
	div#statistic
		div.statistic-info#listened Listened: {{-listened}}
		div.statistic-info#playlists Playlists: {{-playlists.length}}
		div.statistic-info#followers Followers: {{-followers.length}}
		div.statistic-info#followings Followings: {{-followings.length}}
		div.statistic-info#liked Liked: {{-liked.length}}
